{
  "name": "webapp",
  "version": "2.5.0",
  "author": "TISTA Science & Technology Corporation (https://github.com/TISTATechnologies)",
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "d3": "^5.15.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-geolocation": "^1.0.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "bash ./scripts/build-webapp.sh",
    "update-config": "bash ./scripts/update-config-json.sh",
    "build:webapp": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "bash ./scripts/deploy-webapp.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "ignorePatterns": [
      "serviceWorker.js"
    ],
    "rules": {
      "no-unused-vars": 1,
      "linebreak-style": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-props-no-spreading": 0,
      "react/prop-types": 0,
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.0"
  }
}
